{
  "name": "react-redux-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@semantic-release/git": "^6.0.1",
    "cross-env": "^5.1.4",
    "cypress": "^3.0.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.1.1",
    "semantic-release": "^15.6.0",
    "semantic-release-docker": "^2.1.0"
  },
  "dependencies": {
    "history": "^4.6.3",
    "marked": "^0.3.6",
    "prop-types": "^15.5.10",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4100 react-scripts start",
    "build": "react-scripts build",
    "test:e2e": "cypress run",
    "test:unit": "cross-env PORT=4100 react-scripts test --env=jsdom",
    "test": "npm-run-all -s test:unit -p -r start test:e2e",
    "eject": "react-scripts eject"
  },
  "release": {
    "verifyConditions": [
      "semantic-release-docker"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "publish": {
      "path": "semantic-release-docker",
      "name": "leosuncin/realworld-frontend"
    }
  }
}
